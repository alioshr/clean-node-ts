{
  "name": "clean-express",
  "version": "1.1.1",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "rimraf dist && tsc",
    "start": "node dist/main/server.js",
    "debug": "nodemon --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "dev": "nodemon -x ts-node src/main/server.ts",
    "up": "yarn run heroku-postbuild && docker-compose up",
    "down": "docker-compose down",
    "type-check": "tsc --noEmit",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage --no-cache",
    "test:coveralls": "yarn run test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Aliosh Romano Pimenta",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.13",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^4.8.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.1.0",
    "nodemon": "^2.0.21",
    "validator": "^13.9.0"
  },
  "resolutions": {
    "uuid": "^8.3.2"
  }
}
